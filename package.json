{
  "name": "serverless-nestjs",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --path tsconfig.build.json --webpack --webpackPath webpack-config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "prisma migrate deploy && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:local": "cp .env.local .env && npm run build && sls offline",
    "setup:e2e": "cp .env.e2e .env; mysql -h127.0.0.1 -uroot -pexample --force -e'DROP DATABASE demo_service_e2e' 2> /dev/null; prisma migrate deploy",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run setup:e2e && jest --runInBand --config ./test/jest-e2e.json",
    "deploy:stage": "cp .env.stage .env && npm run build && serverless deploy"
  },
  "dependencies": {
    "@jest/test-sequencer": "^28.1.1",
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/common": "^8.1.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/swagger": "^5.2.1",
    "@prisma/client": "^3.15.1",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nest-winston": "^1.6.2",
    "nestjs-prisma-crud": "^1.0.0-beta.15",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.3.0",
    "ts-pattern": "^4.0.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/aws-lambda": "^8.10.59",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.4.1",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "jest": "^27.3.0",
    "mysql": "^2.18.1",
    "nestjs-prisma-crud-schematics": "^1.0.0-beta.11",
    "prettier": "^2.4.1",
    "prisma": "^3.15.2",
    "run-script-webpack-plugin": "^0.1.1",
    "serverless-localstack": "^0.4.36",
    "serverless-offline": "^6.9.0",
    "serverless-sam": "^0.2.0",
    "serverless-webpack": "^5.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.4.4",
    "webpack": "^5.73.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
